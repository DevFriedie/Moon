 --Tabs--

 local Tab = FiveNight:CreateTab("Main")

 local Section = Tab:CreateSection("Main Settings")

 local Slider = Tab:CreateSlider({
	Name = "WalkSpeed",
	Range = {16, 250},
	Increment = 10,
	Suffix = "WalkSpeed",
	CurrentValue = 10,
	Flag = "",
	Callback = function(v)
		getgenv().game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = v
	end,
 })

 local Slider = Tab:CreateSlider({
	Name = "JumpPower",
	Range = {50, 500},
	Increment = 10,
	Suffix = "JumpPower",
	CurrentValue = 10,
	Flag = "", 
	Callback = function(v)
		getgenv().game.Players.LocalPlayer.Character.Humanoid.JumpPower = v
	end,
 })

 local Toggle = Tab:CreateToggle({
    Name = "GodMode + ShockWave Mastery 50",
    CurrentValue = false,
    Flag = "Toggle1",
    Callback = function(state)
        if state then
			_G.loop = true
		while _G.loop == true do wait()
            game:GetService("ReplicatedStorage").Remotes.To_Server.Handle_Initiate_S:FireServer("skil_ting_asd",game:GetService("Players").LocalPlayer,"akaza_bda_compass_needle",1)
		end
	else
		_G.loop = false
				while _G.loop == true do wait()
					print('STOP')
				end
	end
    end,
})

 local Toggle = Tab:CreateToggle({
	Name = "Esp",
	Callback = function()
	Esp()
	end,
 })

 local Toggle = Tab:CreateToggle({
    Name = "NoClip",
    CurrentValue = false,
    Flag = "Toggle1",
    Callback = function()
	NoClip()
    end,
})

 local Toggle = Tab:CreateToggle({
    Name = "AntiIdle",
    CurrentValue = false,
    Flag = "Toggle1",
    Callback = function(state)
		if state then
			_G.loop = true
		while _G.loop == true do wait()
			game:GetService("Players").LocalPlayer.Idled:connect(function()
				game:GetService("VirtualUser"):ClickButton2(Vector2.new())
		end)
	end
	else
		_G.loop = false
				while _G.loop == true do wait()
					print('STOP')
				end
	end
	end,
 })

 local Input = Tab:CreateInput({
	Name = "Change Clan",
	PlaceholderText = "What Clan?",
	RemoveTextAfterFocusLost = false,
	Callback = function(Text)
		game.ReplicatedStorage.Player_Data.DevFriedie4.Clan.Value = Text;
	end,
 })

