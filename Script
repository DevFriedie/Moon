local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

local FiveNight = Rayfield:CreateWindow({
	Name = "Moon by the Sea",
	LoadingTitle = "Moon by the Sea",
	LoadingSubtitle = "by DevFriedie",
	ConfigurationSaving = {
	   Enabled = false,
	   FolderName = FiveNight,
	   FileName = "Moon by the Sea"
	},
 })

 --Values--


--// Function
NoClip = function()
	local Player = game:GetService("Players").LocalPlayer
	local Character = Player.Character

	for _, v in ipairs(Character:GetChildren()) do

	end
end

--// Teleport


--// Tabs

 local Tab = FiveNight:CreateTab("Main")

 local Section = Tab:CreateSection("Main Settings")

 local Slider = Tab:CreateSlider({
	Name = "WalkSpeed",
	Range = {16, 250},
	Increment = 10,
	Suffix = "WalkSpeed",
	CurrentValue = 16,
	Flag = "",
	Callback = function(v)
		game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = v
	end,
 })

 local Slider = Tab:CreateSlider({
	Name = "JumpPower",
	Range = {50, 500},
	Increment = 10,
	Suffix = "JumpPower",
	CurrentValue = 50,
	Flag = "", 
	Callback = function(v)
		game.Players.LocalPlayer.Character.Humanoid.JumpPower = v
	end,
 })

 local Toggle = Tab:CreateToggle({
	Name = "Esp",
	Callback = function()
        local Anim = Instance.new("Animation", game.Workspace)
		Anim.Name = "gay"
		Anim.AnimationId = "rbxassetid://282574440"
		local track = game.Workspace.growagardenplay1108.Humanoid:LoadAnimation(Anim)
		track:Play()
	end,
 })

 local Toggle = Tab:CreateToggle({
    Name = "NoClip",
    CurrentValue = false,
    Flag = "",
    Callback = function()
		NoClip()
    end,
})

 local Input = Tab:CreateInput({
	Name = "Input",
	PlaceholderText = "Input",
	RemoveTextAfterFocusLost = false,
	Callback = function(Text)
		game.ReplicatedStorage.Player_Data.DevFriedie4.Clan.Value = Text;
	end,
 })

 local Button = Tab:CreateButton({
	Name = "Destroy Interface",
	Callback = function()
		Rayfield:Destroy()
	end,
 })

 local Tab = FiveNight:CreateTab("AutoFarm")

 local Section = Tab:CreateSection("AutoFarm Settings")

 local Toggle = Tab:CreateToggle({
    Name = "AutoFarm",
    CurrentValue = false,
    Flag = "Toggle1",
    Callback = function()

    end,
})

 local Tab = FiveNight:CreateTab("Teleport")

 local Section = Tab:CreateSection("Teleport Settings")

 local Button = Tab:CreateButton({
	Name = "Random",
	Callback = function()
		local players = game:GetService('Players')
local lplr = players.LocalPlayer
local lastCF, stop, heartbeatConnection
local function start()
    heartbeatConnection = game:GetService('RunService').Heartbeat:Connect(function()
        if stop then
            return 
        end 
        lastCF = lplr.Character:FindFirstChildOfClass('Humanoid').RootPart.CFrame
    end)
    lplr.Character:FindFirstChildOfClass('Humanoid').RootPart:GetPropertyChangedSignal('CFrame'):Connect(function()
        stop = true
        lplr.Character:FindFirstChildOfClass('Humanoid').RootPart.CFrame = lastCF
        game:GetService('RunService').Heartbeat:Wait()
        stop = false
    end)    
    lplr.Character:FindFirstChildOfClass('Humanoid').Died:Connect(function()
        heartbeatConnection:Disconnect()
    end)
end

lplr.CharacterAdded:Connect(function(character)
    repeat 
        game:GetService('RunService').Heartbeat:Wait() 
    until character:FindFirstChildOfClass('Humanoid')
    repeat 
        game:GetService('RunService').Heartbeat:Wait() 
    until character:FindFirstChildOfClass('Humanoid').RootPart
    start()
end)

lplr.CharacterRemoving:Connect(function()
    heartbeatConnection:Disconnect()
end)

start()
			local lp = game.Players.LocalPlayer
			local char = lp.Character
			function bypassTP(cf, t)
				local hrp = char.PrimaryPart
				local ts = game:GetService('TweenService')
				ts:Create(hrp,
				TweenInfo.new(t, Enum["EasingStyle"].Linear),
				{CFrame = cf
				}):Play()
			end
			bypassTP(CFrame.new(-409.58844, -6.50197887, -128.096161), 3)
	end,
 })

